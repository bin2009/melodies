{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "build": {
      "ignore": [
        "**/*.ts(0)",
        ".eslintrc.json",
        ".eslintcache",
        "node_modules/"
      ]
    }
  }
  