{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "eslint": {
      "ignore": [
        ".eslintrc.json",
        ".eslintcache"
      ]
    },
    "typescript": {
      "ignore": [
        "**/*.ts(0)"
      ]
    }
  }
  